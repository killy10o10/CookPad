 <table class="table table-dark table-striped mt-5">
    <thead>
    <tr>
      <th scope="col">Food</th>
      <th scope="col">Quantity</th>
      <th scope="col">Price</th>
    </tr>
    </thead>
    <tbody>
        <% shopping_list_hash.each do |key, value|%>
            <tr>
                <td><%= key %></td>
                <td><%= value[0] %> <%= value[1] %> </td>
                <td>$ <%= value[2] %></td>
            </tr>
        <% end %>
    </tbody>
  </table>